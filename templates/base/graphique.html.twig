{% extends 'base.html.twig' %}
{% block title %}
	{{parent()}}
	Graphique des valeurs des modules
{% endblock %}
{% block body %}
	<div class="container-fluid">
		<div class="row justify-content-center">
			<div class="col-10 text-primary">
				<h1 class="text-center titrepage mt-4 pt-4 display-1 fw-bold">
					Graphique des valeurs des modules
				</div>
			</div>
		</div>
	</div>

	<div id="charts-container">
		{% for module in modules %}
			<div class="chart-container">
				<h2>{{ module.nom }}</h2>
				<canvas id="chart-{{ module.id }}"></canvas>
			</div>
		{% endfor %}
	</div>
	
{% endblock %}

